prompt 1
default menu.c32
timeout 100

label 1. NFS Installation - x86vbox
menu x86vbox_install
kernel x86vbox/kernel
append ip=dhcp initrd=x86vbox/initrd.img root=/dev/nfs rw androidboot.hardware=x86vbox INSTALL=1 SRC=/x86vbox ROOT=192.168.56.1:/home/mcafee/vol1/android-6/out/target/product VBOX_GUEST_ADDITIONS=1 SDCARD=x86vbox DATA=sda2

label 2. x86vbox (ROOT=/dev/sda1, debug)
menu x86vbox_sda1_debug
kernel x86vbox/kernel
append ip=dhcp console=ttyS3,115200 initrd=x86vbox/initrd.img androidboot.hardware=x86vbox DEBUG=2 SRC=/android-x86vbox ROOT=/dev/sda1 VBOX_GUEST_ADDITIONS=1 SDCARD=x86vbox DATA=sda2

label 3. Recovery (ROOT=/dev/sda1, debug) - x86vbox
menu x86vbox_recovery_debug
kernel x86vbox/kernel
append ip=dhcp console=ttyS3,115200 initrd=x86vbox/initrd.img androidboot.hardware=x86vbox DEBUG=2 SRC=/android-x86vbox ROOT=/dev/sda1 VBOX_GUEST_ADDITIONS=1 SDCARD=x86vbox DATA=sda2 RECOVERY=1

label 4. NFS Installation (serial port) - x86vbox
menu x86vbox_install_serial
kernel x86vbox/kernel
append ip=dhcp console=ttyS3,115200 initrd=x86vbox/initrd.img root=/dev/nfs rw androidboot.hardware=x86vbox INSTALL=1 DEBUG=2 SRC=/x86vbox ROOT=192.168.56.1:/home/mcafee/vol1/android-6/out/target/product VBOX_GUEST_ADDITIONS=1 SDCARD=x86vbox DATA=sda2

label 5. x86vbox (ROOT=/dev/sda1)
menu x86vbox_sda1
kernel x86vbox/kernel
append ip=dhcp initrd=x86vbox/initrd.img androidboot.hardware=x86vbox DEBUG=2 SRC=/android-x86vbox ROOT=/dev/sda1 VBOX_GUEST_ADDITIONS=1 SDCARD=x86vbox DATA=sda2



